{
  "name": "goprfaqyou",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && npm run copy-cloudflare-files",
    "copy-cloudflare-files": "node -e \"const fs = require('fs'); ['_headers', '_redirects'].forEach(f => { if (fs.existsSync(f)) fs.copyFileSync(f, 'out/' + f); });\"",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npm run build",
    "pages:deploy": "npx wrangler pages deploy out"
  },
  "dependencies": {
    "framer-motion": "^12.23.6",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-tsparticles": "^2.12.2",
    "tsparticles": "^3.8.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
